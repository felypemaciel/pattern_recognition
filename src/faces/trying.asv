close all

% data = load("recfaces.dat");
% 
% size(data)

% X = data(:,1:end-1)';
% Y = data(:,end);

x = X(:,2);
lamb = 50;
xstar = (x.^lamb - 1)/lamb;
figure; 

mxstar = mean(xstar);
dpxstar = std(xstar);
xnorm = normrnd(mxstar, dpxstar, 400, 1);

e = sqrt(xstar - xnorm);

subplot(2,1,1);
histfit(xstar);
subplot(2,1,2);
cdfplot(xstar);
hold on;
cdfplot(xnorm);

y = cdf('normal',xnorm);
